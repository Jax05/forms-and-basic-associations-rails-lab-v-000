<%= form_for @song do |f| %>
  <%= f.label :title %><br />
  <%= f.text_field :title %>

  <%= f.label :artist_name %><br />
  <%= f.text_field :artist_name %>

  <%= f.collection_select :genre_name, Genre.all, :name, :name %><br />

  <input name="song[song_notes_1][]">
  <input name="song[song_notes_2][]">
  <input name="song[song_notes_3][]">

  <%= f.submit %>
<% end %>
